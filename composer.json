{
  "name": "lessname/database",
  "description": "Handle your data",
  "license": "proprietary",
  "repositories": [
    {
      "url": "https://gitlab.com/api/v4/group/6632935/-/packages/composer/packages.json",
      "type": "composer"
    }
  ],
  "require": {
    "php": "^8.1.0",

    "doctrine/dbal": "^3.1.0",

    "lessname/value-object": "^0.2.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^1.0.0",
    "phpunit/phpunit": "^9.5.0",
    "vimeo/psalm": "^4.10.0"
  },
  "autoload": {
    "psr-4": {
      "LessDatabase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessDatabaseTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
