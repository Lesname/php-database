{
  "name": "lessname/database",
  "license": "MIT",
  "require": {
    "php": "^8.4.0",

    "doctrine/dbal": "^4.0.0",

    "lessname/value-object": "^0.4.0"
  },
  "require-dev": {
    "lessname/domain": "^0.5.0",

    "squizlabs/php_codesniffer": "^3.0.0",
    "phpstan/phpstan": "^2.0.0",
    "phpunit/phpunit": "^10.0.0"
  },
  "autoload": {
    "psr-4": {
      "LessDatabase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessDatabaseTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
